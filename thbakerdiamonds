<script>
    require(['jquery'], function($) {
        'use strict';
        $(document).ready(function() {
            let menuNav = document.querySelector('#amblog-catopen');
            if (menuNav) {
                menuNav.addEventListener('click', function(e) {
                    e.preventDefault();
                    let catlist = document.querySelector('#amblog-catlist');
                    let menuicon = document.querySelector('.menu-icon');
                    if(catlist.clientHeight === 0) {
                        catlist.style.height = catlist.scrollHeight + 'px';
                        menuicon.classList.add('switched');
                    } else {
                        catlist.style.removeProperty('height');
                        menuicon.classList.remove('switched');
                    }
                });
            };
        });
    });
</script>        </div>
