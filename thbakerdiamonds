    klevu.interactive(function () {
        var options = {"powerUp":{"recsModule":true},"recs":{"apiKey":"klevu-163396098244614199"},"analytics":{"apiKey":"klevu-163396098244614199"}};
        <!-- Magento image path needs to be updated if pubIsInUse -->
klevu.coreEvent.attach("setRemoteConfigRecsBaseUpdates", {
    name: "klevuMagentoImage",
    fire: function () {
        klevu.recs.base.getScope().chains.search.control.add({
            name: "klevuMagentoImage",
            fire: function (data, scope) {
                scope.recsScope.searchObject.getScope().chains.template.process.success.add({
                    name: "klevuMagentoImage",
                    fire: function (data, scope) {
                        if (typeof window.klevu_pubIsInUse == "undefined") {
                            window.klevu_pubIsInUse = true;
                        }
                        klevu.each(data.template.query, function (key, query) {
                            if (query.result.length > 0) {
                                klevu.each(query.result, function (key, product) {
                                    if (window.klevu_pubIsInUse) {
                                        product.image = product.image.replace('pub/', '');
                                        product.image = product.image.replace('needtochange/', '');
                                    } else {
                                        product.image = product.image.replace('needtochange/', 'pub/');
                                    }
                                });
                            }
                        });
                    }
                });
            }
        });
    }
});

require([
    'jquery',
    'Amasty_Base/vendor/slick/slick.min'
], function($) {
    klevu.coreEvent.attach("setRemoteConfigRecsBaseUpdates", {
        name: "thbakerMakeSlider",
        fire: function() {
            var parent = $('.klevu-recs');

            checkElementTimer = setInterval(function() {
                if (parent.find('.product-items').length > 0) {
                    triggerSlickJs(parent.find('.product-items'));
                    clearInterval(checkElementTimer);
                }
            }, 250);
        }
    });

    function triggerSlickJs(items) {
        items.slick({
            dots: false,
            infinite: true,
            slidesToShow: 6,
            slidesToScroll: 1,
            responsive: [
                {
                    breakpoint: 1024,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 1
                    }
                },
                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 1
                    }
                },
                {
                    breakpoint: 425,
                    settings: {
